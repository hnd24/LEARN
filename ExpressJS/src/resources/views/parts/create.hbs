<form method='POST' action='/parts/store'>
  <div class='form-group'>
    <label>Name</label>
    <input
      type='text'
      class='form-control'
      name='name'
      placeholder='Enter name'
    />

  </div>
  <div class='form-group'>
    <label>Tuoi</label>
    <input
      type='text'
      class='form-control'
      name='tuoi'
      placeholder='Enter tuoi'
    />
  </div>
  <button type='submit' class='btn btn-primary'>Create</button>
</form>